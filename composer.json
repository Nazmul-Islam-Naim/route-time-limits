{
    "name": "ntimes/route-time-limits",
    "description": "A Laravel package for limiting time spent on routes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nazmul Islam Naim",
            "email": "nazmulislam.bdb@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0", 
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "NTimes\\RouteTimeLimits\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NTimes\\RouteTimeLimits\\RouteTimeLimitsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "route",
        "time",
        "limits",
        "middleware",
        "time-tracking",
        "rate-limiting",
        "user-tracking",
        "access-control"
    ]
}